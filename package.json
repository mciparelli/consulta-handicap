{
  "private": true,
  "sideEffects": false,
  "name": "consulta-handicap",
  "description": "consulta de handicap",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i app/styles/tailwind.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i app/styles/tailwind.css -o app/styles/app.css"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.11",
    "@nivo/core": "^0.80.0",
    "@nivo/line": "^0.80.0",
    "@prisma/client": "^4.5.0",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "@remix-run/vercel": "^1.6.8",
    "cheerio": "^1.0.0-rc.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.0-beta.10",
    "use-debounce": "^8.0.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.8",
    "@remix-run/serve": "^1.6.8",
    "concurrently": "^7.4.0",
    "prisma": "^4.5.0",
    "tailwindcss": "^3.1.8",
    "vercel": "^28.4.12"
  },
  "engines": {
    "node": ">=14"
  }
}
