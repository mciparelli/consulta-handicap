{
  "private": true,
  "sideEffects": false,
  "name": "consulta-handicap",
  "description": "consulta de handicap",
  "scripts": {
    "build": "npm run build:css && remix build",
    "postbuild": "npm run setup",
    "build:css": "tailwindcss -m -i app/styles/tailwind.css -o app/styles/app.css",
    "predev": "npm run setup",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i app/styles/tailwind.css -o app/styles/app.css",
    "deploy": "fly deploy",
    "setup": "prisma db push && prisma generate",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.2",
    "@heroicons/react": "^2.0.11",
    "@nivo/core": "^0.80.0",
    "@nivo/line": "^0.80.0",
    "@prisma/client": "^4.5.0",
    "@remix-run/node": "^1.11.1",
    "@remix-run/react": "^1.11.1",
    "cheerio": "^1.0.0-rc.12",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.0",
    "use-debounce": "^9.0.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.11.1",
    "@remix-run/serve": "^1.11.1",
    "concurrently": "^7.4.0",
    "prisma": "^4.5.0",
    "tailwindcss": "^3.1.8"
  },
  "engines": {
    "node": ">=16"
  }
}
